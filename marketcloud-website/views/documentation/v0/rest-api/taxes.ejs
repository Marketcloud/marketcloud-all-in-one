<!-- TAXES CREATE -->
					<div class="documentation">
						<h3>POST /taxes</h3>
						<p class="lead">Creates a new Tax.</p>
						<pre class="code http-call">POST http://api.marketcloud.it/v0/taxes</pre>
						<h5>Parameters</h5>
						<table class="table table-striped documentation">
									<thead>
										<tr>
											<th>Attribute</th>
											<th>Type</th>
											<th>Description</th>
										</tr>
									</thead>
									<tbody>
										
										<tr>
											<td class="code">name <span  class="label label-success">Required</span></td>
											<td>String</td>
											<td>The tax name</td>
										</tr>
										<tr>
											<td class="code">rate <span class="label label-success">Required</span></td>
											<td>Number</td>
											<td>Percentage value eligible for this tax rule.</td>
										</tr>
										<tr>
											<td>description</td>
											<td>String</td>
											<td>A description of the tax.</td>
										</tr>
									</tbody>
								</table>
						<h5>Example Request</h5>
						<iframe src="//api.apiembed.com/?source=https://raw.githubusercontent.com/Marketcloud/json-request-examples/master/carts/update.json&targets=shell:curl,java:unirest,python:requests,ruby:native,objc:nsurlsession,go:native,csharp:restsharp" frameborder="0" scrolling="no" width="100%" height="300px" seamless></iframe>
						<h5>Example JSON response (200)</h5>
						<pre class="code prettyprint" >
{
	status:true,
	data: {
		id:10,
		name: "Minimum VAT",
		description: "To be applied for first use goods (food, press...)"
	},
	links : {
		self : "http://api.marketcloud.it/v0/taxes/10"
	}
}</pre>								
							</div>
<!-- / TAX CREATE -->						

<!-- TAX LIST -->
						<div class="documentation">
						<h3>GET /taxes</h3>
						<p class="lead">Retrieves a paginated response of a list of taxes from your application.</p>
						<pre class="code http-call">GET http://api.marketcloud.it/v0/taxes</pre>
						<h5>Request parameters</h5>
						<table class="table table-striped documentation">
									<thead>
										<tr>
										<th>Field</th>
										<th>Type</th>
										<th>Description</th>
										</tr>
									</thead>
									<tbody>
								<tr>
									<td class="code">fields</td>
									<td>String</td>
									<td>Comma separated list of attribute names to retrieve. Use it to retrieve only the fields you need.</td>
								</tr>
								<tr>
									<td  class="code">skip</td>
									<td>Number</td>
									<td>How many resources to be skipped</td>
								</tr>
								<tr>
									<td class="code">limit</td>
									<td>Number</td>
									<td>How many resource to be fetched</td>
								</tr>
							</tbody>
								</table>
						<h5>Example Request</h5>
						<iframe src="//api.apiembed.com/?source=https://raw.githubusercontent.com/Marketcloud/json-request-examples/master/carts/update.json&targets=shell:curl,java:unirest,python:requests,ruby:native,objc:nsurlsession,go:native,csharp:restsharp" frameborder="0" scrolling="no" width="100%" height="300px" seamless></iframe>
						<h5>Example JSON response (200)</h5>
						<pre class="code prettyprint" >
{
	status:true,
	data: [ {
		data : {
			id:9,
			name: "Ordinary VAT",
			description: "To be applied when other VAT does not apply"
		},
		links : {
				first : 'http://api.marketcloud.it/v0/taxes/9',
				last : 'http://api.marketcloud.it/v0/taxes/10',
				self : 'http://api.marketcloud.it/v0/taxes/9',
				prev : null,
				next : 'http://api.marketcloud.it/v0/taxes/10'
		}

	},{
		data : {
			id:10,
			name: "Minimum VAT",
			description: "To be applied for first use goods (food, press...)"
		},
		links : {
				first : 'http://api.marketcloud.it/v0/taxes/9',
				last : 'http://api.marketcloud.it/v0/taxes/10',
				self : 'http://api.marketcloud.it/v0/taxes/10',
				prev : 'http://api.marketcloud.it/v0/taxes/9'
				next : null
		}
	}
	]
}</pre>								

						</div>
<!-- / TAX LIST -->						
